''Pins
    interface "pin 1" as pin.1 #Yellow
    interface "pin 2" as pin.2 #Yellow
    interface "pin 3" as pin.3 #Black
    interface "pin 4" as pin.4 #Black
    interface "pin 5" as pin.5 #Blue
    interface "pin 6" as pin.6 #Blue
    interface "pin 7" as pin.7 #Blue
    interface "pin 8" as pin.8 #Blue
    interface "pin 9" as pin.9 #Blue
    interface "pin 10" as pin.10 #Blue
    interface "pin 11" as pin.11 #Blue
    interface "pin 12" as pin.12 #Blue
    interface "pin 13" as pin.13 #Orange
    interface "pin 14" as pin.14 #Black
    interface "pin 15" as pin.15 #Gray
    interface "pin 16" as pin.16 #Red
    interface "pin 17" as pin.17 #Green
    interface "pin 18" as pin.18 #Green
    interface "pin 19" as pin.19 #Green
    interface "pin 20" as pin.20 #Green
    interface "pin 21" as pin.21 #Purple
    interface "pin 22" as pin.22 #Purple
    interface "pin 23" as pin.23 #Purple
    interface "pin 24" as pin.24 #Blue

''binding first pin of row1 to first pin of row2
pin.1 -[hidden]> pin.13
pin.2 -[hidden]> pin.14
pin.3 -[hidden]> pin.15
